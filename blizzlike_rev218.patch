-        return false;
-
-    while (!feof(file))
-    {
-        line[0] = 0x0;
-        fgets(line, 1020, file);
-
-        // check for UTF8 prefix and comments
-        if (strlen(line) >= 3)
-            if (line[0] == '\xEF' && line[1] == '\xBB' && line[2] == '\xBF')
