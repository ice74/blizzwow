-        if (line_s == "")
-            continue;
-
-        // process line without CR/LF
-        line_s = line; 
-        line_s = trim(line_s, "\x0A\x0D");
-    
-        pos = 0;
-        if (ReadUTF8(line_s, lchar, pos))
-        {
-            // create analogs vector
-            LC_AnalogVector av;
-            while (ReadUTF8(line_s, lanalog, pos))
