-bool LexicsCutter::ReadUTF8(std::string& in, std::string& out, unsigned int& pos)
-{
-    if (pos >= in.length())
-        return false;
-
-    out = "";
-    unsigned char c = in[pos++];
-    out += c;
-    int toread = trailingBytesForUTF8[(int) c];
-    while ((pos < in.length()) && (toread > 0))
-    {
-        out += in[pos++];
-        toread--;
-    }
-
-    return true;
-}
